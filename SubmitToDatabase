StrSQL = "INSERT INTO [Nozzle Readings] ([Nozzle ID],[Closing Reading], [Testing Ltrs]) VALUES(2,txtnozzle2reading.Value,txtnozzle2test.Value);"
DoCmd.RunSQL StrSQL
StrSQL = "INSERT INTO [Nozzle Readings] ([Nozzle ID],[Closing Reading], [Testing Ltrs]) VALUES(3,txtnozzle3reading.Value,txtnozzle3test.Value);"
DoCmd.RunSQL StrSQL
StrSQL = "INSERT INTO [Nozzle Readings] ([Nozzle ID],[Closing Reading], [Testing Ltrs]) VALUES(4,txtnozzle4reading.Value,txtnozzle4test.Value);"
